<body class="cwps-body" style="margin:0px;padding:0px;overflow:hidden;height:100vh;">
    <partial name="_CosmosMainMenuPartial" />
    <iframe id="iFrameWebPage" src="@frameUrl" style="position: absolute; top: 52px; bottom: 0px; left: 0px; right: 0px; width: 100%; height: calc(100vh - 52px); margin:0px; padding:0px; border:none; z-index:1;"></iframe>
    <script>
        // The following is used to wireup the menu links to this page.
        function setupIframe() {
            const iframe = document.getElementById("iFrameWebPage");
            const iframeWin = iframe.contentWindow || iframe.contentDocument.parentWindow;
            if (iframeWin.document.body) {
                iframe.height = window.innerHeight - document.getElementById("ccmsNavMenu").clientHeight;// Create a style element
                const link = iframeWin.document.createElement('link');
                link.rel = 'stylesheet';
                link.type = 'text/css';
                link.href = '/css/hangfire-dark.css';
                iframeWin.document.head.appendChild(link);
            }
        };
        window.addEventListener('resize', function (event) { setupIframe(); }, true);

        $(document).ready(function() {
            var iframe = $('#iFrameWebPage');
            iframe.on('load', function() {
                setupIframe();
            });

