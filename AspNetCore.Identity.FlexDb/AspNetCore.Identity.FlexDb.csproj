<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Version>9.2.1.0</Version>
		<AssemblyVersion>9.2.1.0</AssemblyVersion>
		<FileVersion>9.2.1.0</FileVersion>
		<PackageVersion>9.2.1.0</PackageVersion>
		<Authors>toiyabe62</Authors>
		<Nullable>enable</Nullable>
		<RepositoryType>GitHub</RepositoryType>
		<PackageTags>Cosmos, CosmosDB, Identity, Provider, AspNetCore, Membership, Roles, AspNetCore.Identity, AspNetCore.Identity.CosmosDb, Microsoft.AspNetCore.Identity, aspnet, efcore, Azure Cosmos DB</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageId>AspNetCore.Identity.FlexDb</PackageId>
		<RepositoryUrl>https://github.com/CosmosSoftware/AspNetCore.Identity.FlexDb</RepositoryUrl>
		<PackageProjectUrl>https://github.com/CosmosSoftware/AspNetCore.Identity.FlexDb</PackageProjectUrl>
		<PackageIcon>B2C_BrandIcon_Black.png</PackageIcon>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<RepositoryType>git</RepositoryType>
		<Description>A Cosmos DB, MySQL and MS SQL implementation of ASP.NET Core Identity. The choice of DB provider is automatically chosen by the connection string.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<Company>Cosmos Software LLC</Company>
		<Copyright>Moonrise Software LLC</Copyright>
		<PackageReleaseNotes>Dependencies update.</PackageReleaseNotes>
		<NoWin32Manifest>true</NoWin32Manifest>
		<Title>ASP.NET Core Identity Provider for Azure Cosmos DB</Title>
	</PropertyGroup>

	<PropertyGroup>
	  <RestorePackagesWithLockFile>true</RestorePackagesWithLockFile>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" />
        <PackageReference Include="Microsoft.AspNetCore.Identity.UI" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Cosmos" />
		<PackageReference Include="Microsoft.Data.Sqlite.Core" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" />
		<PackageReference Include="Pomelo.EntityFrameworkCore.MySql" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="B2C_BrandIcon_Black.png" />
		<Content Include="_res\icons\B2C_BrandIcon_Black.png" />
		<PackageReference Include="Duende.IdentityServer.EntityFramework.Storage" />
		<PackageReference Include="SQLitePCLRaw.bundle_e_sqlcipher" />
	</ItemGroup>

	<ItemGroup>
		<None Include="_res\icons\nuget-icon.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <None Update="B2C_BrandIcon_Black.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
